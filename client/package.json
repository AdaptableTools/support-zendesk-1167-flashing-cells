{
  "scripts": {
    "start": "npm run config:local && vite --port 8080",
    "preview": "npm run config:local && vite build && vite preview --port 8080",
    "config:local": "shx cp env_configs/config.local.js public/config/config.js",
    "no:config": "shx cp env_configs/noconfig.js public/config/config.js",
    "build": "npm run lint && npm run check:types && npm run no:config && vite build",
    "clean": "shx rm -rf build",
    "compile:configs": "tsc env_configs_sources/config.development.ts --outDir env_configs/",
    "lint": "eslint \"src/**/*.+(ts|tsx|html)\" && stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
    "lint:fix": "eslint \"src/**/*.+(ts|tsx|html)\" --fix && stylelint \"src/**/*.scss\" --custom-syntax postcss-scss --fix",
    "check:types": "tsc --noemit",
    "test": "jest",
    "test:debug": "cross-env DEBUG_PRINT_LIMIT=1000000 jest --detectOpenHandles",
    "test:cover": "jest --coverage",
    "clean:modules": "shx rm -rf node_modules",
    "clean:modules:lock": "npm run clean:modules && shx rm -rf package-lock.json",
    "upgrade": "npm run clean:modules && npx npm-check-updates -u && npm update",
    "debug:vite": "cross-env VITE_CJS_TRACE=true vite dev"
  },
  "dependencies": {
